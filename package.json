{
  "name": "json-calendar",
  "version": "1.9.0",
  "description": "A data model for displaying dates and date ranges on a calendar interface.",
  "keywords": [
    "json",
    "calendar",
    "date",
    "date picker",
    "date range"
  ],
  "homepage": "https://github.com/rxgx/json-calendar",
  "license": "MIT",
  "author": "Ryan Gasparini <ryan@rxgx.com> (http://www.rxgx.com)",
  "bugs": {
    "url": "https://github.com/rxgx/json-calendar/issues"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "ts-standard --fix",
    "lint": "ts-standard",
    "release": "semantic-release",
    "test": "jest --coverage",
    "verify": "npm run lint && npm t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxgx/json-calendar.git"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.14",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {},
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "ts-standard": {
    "ignore": "lib"
  },
  "volta": {
    "node": "14.12.0"
  }
}
