{
  "name": "@rxgx/json-calendar",
  "version": "1.8.7",
  "description": "A data model for displaying dates and date ranges on a calendar interface.",
  "keywords": [
    "json",
    "calendar",
    "date",
    "date picker",
    "date range"
  ],
  "homepage": "https://github.com/rxgx/json-calendar",
  "license": "MIT",
  "author": "Ryan Gasparini <ryan@rxgx.com> (http://www.rxgx.com)",
  "bugs": {
    "url": "https://github.com/rxgx/json-calendar/issues"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts src",
    "prepublishOnly": "npm run build",
    "release": "standard-version --dry-run",
    "test": "jest",
    "verify": "npm run lint && npm t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxgx/json-calendar.git"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "prettier": "2.0.5",
    "semantic-release": "^17.0.7",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {},
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator"
    ]
  }
}
